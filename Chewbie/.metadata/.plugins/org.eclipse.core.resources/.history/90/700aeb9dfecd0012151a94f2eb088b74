package me.xiangchen.app.eerv2;

import android.graphics.Point;

public class xacTouch {

	Point start;
	Point end;
	
	public void setStart(Point s) {
		start = s;
	}
	
	public Point getStart() {
		return start;
	}
	
	public void setEnd(Point e) {
		end = e;
	}
	
	public Point getEnd() {
		return end;
	}
}
